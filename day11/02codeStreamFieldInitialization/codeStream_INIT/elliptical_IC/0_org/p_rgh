FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    up      
    {
       	type		zeroGradient;
    }

    "(left|right)"     
    {
        type            zeroGradient;
    }

    down
   {
     type            zeroGradient;
    
    }
    frontAndBack    
    {
        type            empty;
    }
}

