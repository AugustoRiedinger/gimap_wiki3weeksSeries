FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.phase1 0
);

regions
(
    cylinderToCell
    {
	p1 (0.02 0.02 -10);
	p2 (0.02 0.02 10);
	radius 0.01;
        fieldValues
        (
            volScalarFieldValue alpha.phase1 1
        );
    }
);

