FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "1.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(1.0000004 1.0000027 1.0000105 1.0000404 1.0002172 1.000723 1.0023396 1.0120801 1.071796 1.2498533);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


