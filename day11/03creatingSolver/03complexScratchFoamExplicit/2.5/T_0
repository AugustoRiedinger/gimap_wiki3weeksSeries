FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "2.5";
    object      T_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(1 1.0000002 1.0000011 1.0000023 1.0000122 0.99996872 1.0014657 1.0043435 1.0725945 1.2515964);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


