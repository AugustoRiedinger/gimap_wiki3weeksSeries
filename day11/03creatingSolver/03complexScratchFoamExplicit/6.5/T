FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "6.5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(0.99999999 0.99999992 0.99999967 0.99999762 0.99998526 1.0001947 1.0011303 1.0040084 1.0719787 1.2509335);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


