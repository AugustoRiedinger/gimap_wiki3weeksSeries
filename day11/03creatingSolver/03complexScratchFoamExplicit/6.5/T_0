FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "6.5";
    object      T_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(1 1.0000001 1.000001 1.0000019 1.0000096 0.99996244 1.0015687 1.0048836 1.0724456 1.2516293);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


