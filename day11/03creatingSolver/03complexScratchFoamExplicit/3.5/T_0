FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "3.5";
    object      T_0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(1.0000003 1.0000016 1.0000094 1.0000302 1.0000923 1.0005925 1.0017874 1.0121899 1.0715215 1.2501149);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


