FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "3";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   nonuniform List<scalar> 10(1.0000005 1.0000026 1.0000142 1.0000503 1.0001746 1.0006473 1.0027236 1.0117619 1.0715187 1.250178);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    bottom
    {
        type            slip;
    }
    top
    {
        type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}


