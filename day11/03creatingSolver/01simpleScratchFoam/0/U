FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (2.5 0 0);

boundaryField
{
    inlet
    {
        type            zeroGradient;
	//type            fixedValue;
	//value           uniform (2.5 0 0);
    }

    outlet
    {
        type            zeroGradient;
	//type            fixedValue;
	//value           uniform (2.5 0 0);
    }

    top
    {
        type            slip;
    }

    bottom
    {
        type            slip;
    }

    frontAndBack
    {
        type            empty;
    }
}

